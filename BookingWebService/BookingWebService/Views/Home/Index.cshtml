@using BookingWebService.Models
@model List<HotelNumber>

@{
    Layout = "_Layout";
}

<h2>Hello World!</h2>
@foreach (var hotelNumber in Model)
{

    <div>
        <p>ID: @hotelNumber.Id</p>
        <p>Hotel: @hotelNumber.Hotel.Name</p>
        <img src="@string.Format("data:image/png;base64,{0}", hotelNumber.Images[0].ImageData)">
    </div>
}

<script src="~/js/bootstrap.bundle.min.js"></script>

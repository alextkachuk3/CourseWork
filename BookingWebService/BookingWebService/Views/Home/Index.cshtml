@using BookingWebService.Models
@model List<HotelNumber>

@{
    Layout = null;
}
<!doctype html>
<html>
<head>
    <title>Hotel Web Service</title>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="~/css/bootstrap.min.css" />
</head>
<body>
    <h2>Hello World!</h2>
     @foreach(var hotelNumber in Model)
     {
        <div>
            <p>ID: @hotelNumber.Id</p>
        </div>        
     }

     <script src="~/js/bootstrap.bundle.min.js"></script>
</body>
</html>